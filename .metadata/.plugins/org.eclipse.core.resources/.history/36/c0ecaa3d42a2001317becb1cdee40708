package ch20.recursion;

public class exercise23 {

	public static int binaryToDecial(String binaryString) {
		return binaryToDecial(binaryString, ans, 0);
	}
	public static int binaryToDecial(String binaryString, int ans, int index) {
		if (binaryString.charAt(index) == 0)
			return binaryToDecial();
	}
	
}
